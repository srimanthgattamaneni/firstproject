#include <stdio.h>
#include <stdlib.h>

int main()
{
    int marks;
    printf("enter your marks");
    scanf("%d",&marks);

    if (marks<40)
    {
        printf("Grade F ");
    }
    else if (marks>=85)
    {
         printf("Grade A ");
    }
     else if (marks>=70)
    {
         printf("Grade B ");
    }
     else if (marks>=55)
    {
         printf("Grade C ");
    }
     else if (marks>40)
    {
         printf("Grade D ");
    }




    return 0;
}
